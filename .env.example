APP_NAME=TDFC Companion App
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://your-app-url.example

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stderr
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info

DB_CONNECTION=sqlsrv
DATABASE_URL=
DB_HOST=127.0.0.1
DB_PORT=11433
DB_DATABASE=<db-name>
DB_USERNAME=<db-username>
DB_PASSWORD=<db-password>
DB_ENCRYPT=yes
DB_TRUST_SERVER_CERTIFICATE=false
DB_CONNECT_TIMEOUT=10
DB_LOGIN_TIMEOUT=10
DB_QUERY_TIMEOUT=30

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=r2
FILESYSTEM_CLOUD=r2
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=<r2-access-key-id>
AWS_SECRET_ACCESS_KEY=<r2-secret-access-key>
AWS_DEFAULT_REGION=auto
AWS_BUCKET=<r2-bucket-name>
AWS_ENDPOINT=https://<accountid>.r2.cloudflarestorage.com
AWS_USE_PATH_STYLE_ENDPOINT=true
AWS_URL=
R2_REGION=auto
R2_ACCESS_KEY_ID=<r2-access-key-id>
R2_SECRET_ACCESS_KEY=<r2-secret-access-key>
R2_ENDPOINT=<cloudflare-r2-endpoint>
R2_BUCKET=<r2-bucket-name>
R2_PUBLIC_BASE_URL=<public-r2-base-url>
R2_USE_PATH_STYLE_ENDPOINT=true

INERTIA_SSR_ENABLED=false
INERTIA_SSR_URL=http://127.0.0.1:13714
INERTIA_SSR_BUNDLE=
SSR_HOST=127.0.0.1
SSR_PORT=13714

TS_AUTHKEY=<set-in-railway-variables>
TS_HOSTNAME=railway-app
TS_TAGS=tag:railway-app
TAILNET_DB_HOST=<tailnet-db-hostname-or-100.x-address>
TAILNET_DB_PORT=1433
LOCAL_DB_PORT=11433
TS_SOCKS5_PORT=1055

RUN_MIGRATIONS=false

VITE_APP_NAME="${APP_NAME}"
